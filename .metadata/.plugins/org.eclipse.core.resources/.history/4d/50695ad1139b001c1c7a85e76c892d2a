import java.util.Scanner;

public class mainprt19 {
public static void main(String[] args) {
	Scanner sc=new Scanner(System.in);
	System.out.println("Enter the values of N:");
	int n=sc.nextInt();
	for(int i=1;i<=n;i++)
	{
		for(int i=1;j<-n-i;j++)
		{
			System.out.print(" ");
		}
		System.out.println();
	}
}
}
